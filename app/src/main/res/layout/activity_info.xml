<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="de.hawhamburg.sea2.echarge.info">

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/viewVorname"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textStyle="bold"
        android:textSize="20dp" />

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/viewNachname"
        android:layout_below="@+id/viewVorname"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="20dp"
        android:textStyle="bold"
        android:textSize="20dp" />

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/viewGebDatum"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/viewNachname"
        android:layout_alignParentLeft="true"
        android:layout_marginTop="20dp"
        android:textStyle="bold"
        android:textSize="20dp" />

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/viewStrasse"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/viewGebDatum"
        android:layout_alignParentLeft="true"
        android:layout_marginTop="20dp"
        android:textStyle="bold"
        android:textSize="20dp" />

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/viewHausnummer"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/viewStrasse"
        android:layout_alignParentLeft="true"
        android:layout_marginTop="20dp"
        android:textStyle="bold"
        android:textSize="20dp" />

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/viewPLZ"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/viewHausnummer"
        android:layout_alignParentLeft="true"
        android:layout_marginTop="20dp"
        android:textStyle="bold"
        android:textSize="20dp" />

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/viewStadt"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/viewPLZ"
        android:layout_alignParentLeft="true"
        android:layout_marginTop="20dp"
        android:textStyle="bold"
        android:textSize="20dp" />

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/viewLand"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/viewStadt"
        android:layout_alignParentLeft="true"
        android:layout_marginTop="20dp"
        android:textStyle="bold"
        android:textSize="20dp" />

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/viewEmail"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/viewLand"
        android:layout_alignParentLeft="true"
        android:layout_marginTop="20dp"
        android:textStyle="bold"
        android:textSize="20dp" />

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/viewHandynummer"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/viewEmail"
        android:layout_alignParentLeft="true"
        android:layout_marginTop="20dp"
        android:phoneNumber="true"
        android:textStyle="bold"
        android:textSize="20dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Vorname"
        android:id="@+id/testVorname"
        android:layout_below="@+id/viewVorname"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textColor="#2db7ff"
        android:textStyle="italic" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Nachname"
        android:id="@+id/textNachname"
        android:layout_below="@+id/viewNachname"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textColor="#2db7ff"
        android:textStyle="italic" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Geburtsdatum"
        android:id="@+id/textGeburtstag"
        android:layout_below="@+id/viewGebDatum"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textColor="#2db7ff"
        android:textStyle="italic" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="StraÃŸe"
        android:id="@+id/textStrasse"
        android:layout_below="@+id/viewStrasse"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textColor="#2db7ff"
        android:textStyle="italic" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Hausnummer"
        android:id="@+id/textHausnummer"
        android:layout_below="@+id/viewHausnummer"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textColor="#2db7ff"
        android:textStyle="italic" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Postleitzahl"
        android:id="@+id/textPLZ"
        android:layout_below="@+id/viewPLZ"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textColor="#2db7ff"
        android:textStyle="italic" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Stadt"
        android:id="@+id/textStadt"
        android:layout_below="@+id/viewStadt"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:singleLine="false"
        android:textColor="#2db7ff"
        android:textStyle="italic" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Land"
        android:id="@+id/textLand"
        android:layout_below="@+id/viewLand"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textColor="#2db7ff"
        android:textStyle="italic" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="eMail"
        android:id="@+id/textEmail"
        android:layout_below="@+id/viewEmail"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textColor="#2db7ff"
        android:textStyle="italic" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Handynummer"
        android:id="@+id/textHandynummer"
        android:layout_below="@+id/viewHandynummer"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textColor="#2db7ff"
        android:textStyle="italic" />

</RelativeLayout>
